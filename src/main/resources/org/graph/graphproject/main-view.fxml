<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="org.graph.graphproject.GraphController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Button fx:id="generateButton" text="Generate test" onAction="#onGenerateButtonClick"/>

    <TextArea fx:id="outputArea" VBox.vgrow="ALWAYS" editable="false" wrapText="true"/>
</VBox>
